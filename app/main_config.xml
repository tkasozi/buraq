<?xml version="1.0"?>
<configuration>
    <Title>Buraq</Title>
    <Version>Buraq x.x.x</Version>
    <AppLogo>:/icons/workflow</AppLogo>
    <window
        minWidth=""
        minHeight="720"
        normalSize="1200"
        minimizeIcon=":/icons/minimize_icon"
        maximizeIcon=":/icons/maximize_icon"
        restoreIcon=":/icons/restore_icon"
        closeIcon=":/icons/close_icon" />
    <AppIcons>
        <terminal>:/icons/terminal_icon</terminal>
        <settings>:/icons/settings_icon_png</settings>
        <folder>:/icons/folder_icon</folder>
        <playCode>:/icons/play_filled_icon</playCode>
        <execute>:/icons/play_filled_icon2</execute>
        <executeSelected>:/icons/play_filled_icon2</executeSelected>
        <addFile>:/icons/create_new_icon</addFile>
    </AppIcons>
    <Styles>
        <commonStyle>
            <color>#C2C2C2</color>
            <backgroundColor>none</backgroundColor>
            <padding>0px</padding>
            <border>none</border>
            <height>32px</height>
        </commonStyle>
        <controlToolBar inherits="commonStyle">
            <borderColor>1px solid #383838</borderColor>
        </controlToolBar>
        <toolBar inherits="commonStyle">
            <borderBottom>1px solid #383838</borderBottom>
            <height>32px</height>
        </toolBar>
        <statusToolBar inherits="commonStyle">
            <borderTop>1px solid #383838</borderTop>
            <height>24px</height>
        </statusToolBar>
        <toolBarHover>
            <backgroundColor>#383838</backgroundColor>
            <border>0px</border>
        </toolBarHover>
    </Styles>
</configuration>